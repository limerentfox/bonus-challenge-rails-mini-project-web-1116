<h1> <%= @office.name %></h1>
<ul>
    <li>
    <strong>Address:</strong><br>

    <a href="<%= @office.maps_url %>" target="_new">
      <%= @office.street %><br>
      <%== "#{@office.suite} <br />" if @office.suite == " " %>
      <%= @office.city_state_zip%>
    </a>

  </li>

  <p></p>

  <li>
    <strong>Phone:</strong><br>
      <%= @office.phone%>
  </li>

  <p></p>

  <%== "<li>
    <strong>Fax:</strong><br>
      #{@office.fax} <br>
  </li>" if @office.fax == " " %>

  <p></p>

</ul>

<p><%= button_to "Edit Info", edit_office_path(@office), method: :get %></p>
<p><%= button_to "Delete Office", @office, method: :delete %></p>
